class Program{
    int arr[150]; 

    void main(){
        int length, ansmx, ansmn;
        //length = read();
        //for id = 0, length{
        //    arr[id] = read();
        //}
        length = 10;
        arr[0] = 1;arr[1] = 2;arr[2] = 3;arr[3] = 4;arr[4] = 5;arr[5] = 6;arr[6] = 7;arr[7] = 8;arr[8] = 9;arr[9] = 10;
        ansmx = mx(length);
        ansmn = mn(length);
        callout("printf", "Max number: %d | Min number: %d\n", ansmx, ansmn);
    }

    int mx(int length){
        int mmx;
        mmx = arr[0];
        for id = 0, length{
            if(mmx < arr[id]){
                mmx = arr[id];
            }
        }
        return mmx;
    }

    int mn(int length){
        int mmn;
        mmn = arr[0];
        for id = 0, length{
            if(mmn > arr[id]){
                mmn = arr[id];
            }
        }
        return mmn;
    }
}
