class Program{
    int arr[150]; 

    void pr(int length){
        int id;
        for id = 0, length{
            if((arr[id] % 2) == 0){
                callout("printf", "%d ", arr[id]);
            }
            id+=1;
        }
    }

    void main(){
        int length;
        length = 10;
        arr[0] = 1;arr[1] = 2;arr[2] = 3;arr[3] = 4;arr[4] = 5;arr[5] = 6;arr[6] = 7;arr[7] = 8;arr[8] = 9;arr[9] = 10;
        pr(length);
    }

}
