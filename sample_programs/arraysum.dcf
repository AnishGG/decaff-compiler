class Program{
    int arr[150];

    int sm(int length){
        int sum, id;
        sum = 0;
        for id = 0, length{
            sum += arr[id];
            id += 1;
        }
        return sum;
    }

    void main(){
        int length, ans;
        length = 10;
        arr[0] = 1;arr[1] = 2;arr[2] = 3;arr[3] = 4;arr[4] = 5;arr[5] = 6;arr[6] = 7;arr[7] = 8;arr[8] = 9;arr[9] = 10;
        ans = sm(length);
        callout("printf", "%d\n", ans);
    }

}
